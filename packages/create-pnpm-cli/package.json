{
  "name": "create-pnpm-cli",
  "version": "0.0.13",
  "description": "create pnpm cli",
  "scripts": {
    "dev": "tsup ./src/cli.ts --dts --clean --shims --format=cjs --platform=node --outDir=./lib --watch",
    "build": "tsup ./src/cli.ts --dts --clean --shims --format=cjs --platform=node --outDir=./lib"
