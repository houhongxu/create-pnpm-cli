    "./bin"
  ],
  "dependencies": {
    "@inquirer/prompts": "^5.3.6",
    "commander": "^11.1.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "package-manager-install": "^0.0.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "tsup": "^8.2.3"
  },
  "engines": {
    "node": ">=18"
